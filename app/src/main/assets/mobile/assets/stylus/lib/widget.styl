::-webkit-scrollbar{
  display:none;
}
.yoyaLoading{
  overflow:hidden;
  position:fixed;
  text-align:center;
  z-index:4000000000; 
  width:100%;
  left:0;
  top:0;
  bottom:0;
  pointer-events: none;
  s{
    display:block;
    opacity:0.2;
    position:absolute;
    width:100%;
    height:100%;
    z-index:-1;
    background:#000;
    pointer-events: none;
  }
  .loading-content{
    width:100%;
    height:rem(90);
    position:absolute;
    left:0;
    top:50%;
    marign-top:rem(-45);
    .spinner{
      width: rem(90);
      height: rem(90);
      position:relative;
      .double-bounce1, .double-bounce2 {
        width: 100%;
        height: 100%;
        border-radius:100%;
        background:#FFF;
        opacity: 0.8;
        position: absolute;
        top: 0;
        left: 0;
        animation: bounce 1s 2.0s infinite ease-in-out;
      }
      .double-bounce2 {
        animation-delay: -1.0s;
      }
    }
    .spinner,span{
      display:inline-block;
      vertical-align:middle;
    }
    span{
      color:#FFF;
      font-size:rem(30);
      margin:0 0 0 rem(10);
    }
  }

  
}
.popup {
  position: fixed;
  z-index: 300;
  display: table;
  width: 100%;
  height: 100%;
  text-align: center;
  left: 0;
  bottom: 0;
  top: 0;
  right: 0;
  visibility: hidden;
  s {
    position: fixed;
    z-index: -1;
    display: block;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: #000;
    opacity: 0.6;
  }
  .popup-wrap {
    display: table-cell;
    vertical-align: middle;
    .popup-content {
      position: relative;
      padding:0 rem(50);
      min-height: rem(280);
      box-sizing: border-box;
      margin: 0 auto;
      visibility:hidden;
      s {
        position: absolute;
        z-index: -1;
        left: 0;
        right: 0;
        top: 0;
        bottom: 0;
        width: 100%;
        height: 100%;
        display: block;
        background: #000;
        opacity: 0.6;
        border-radius: rem(12);
      }
    }
  }
  &.enter {
    visibility: visible;
    .popup-content {
      visibility: visible;
      animation: fadIn 0.2s
    }
  }

  &.out .popup-content {
    visibility:hidden;
    animation: fadOut 0.2s
  }
  &.tips {
    z-index: 31000;
    .popup-content {
      color: #FFF;
      width:auto;
      min-width:rem(400);
      max-width:rem(500);
      min-height:rem(45);
      padding: rem(50) rem(25);
      s{
        background:#323232;
        opacity:1;
      }
      .des {
        font-size: rem(30);
        line-height: rem(45);
        color: #FFF;
      }
    }
  }

  &.dialog {
    z-index: 30000;
    .popup-content {
      width:rem(540);
      font-size: rem(24);
      padding-bottom: rem(80);
      s {
        background: #FFF;
        opacity: 1;
      }
      .title {
        line-height: rem(80);
        font-size: rem(34);
        color:#333;
      }
      .des {
        padding: rem(15);
        box-sizing: border-box;
        font-size:rem(28);
        line-height: rem(40)
      }
      .dialog-btn {
        position: absolute;
        bottom: 0;
        right: 0;
        left: 0;
        height: rem(90);
        border-top: 1px solid #dbdbdb;
        clearfix();
        label {
          float:left;
          width: 50%;
          box-sizing: border-box;
          button,span.button,a {
            display: block;
            width: 100%;
            height: 100%;
            line-height: rem(90);
            background: none;
            border: none;
            color: #707070;
            font-size: rem(32);
            border-bottom-left-radius: rem(20);
            border-bottom-right-radius: rem(20);
          }
          &:nth-child(2) {
            border-left: 1px solid #dbdbdb;
            button{
              color:#11b6f2;
            }
            
          }
        }
      }
    }

  }

}

 
@keyframes bounce {
  0%, 100% {
    transform: scale(0.0);
    -webkit-transform: scale(0.0);
  }
  50% {
    transform: scale(1.0);
    -webkit-transform: scale(1.0);
  }
}
@keyframes fadIn {
  from {
    //visibility:hidden;
    transform: scale(0)
    opacity: 0;
  }
  to {
    //visibility:visible;
    transform: scale(1)
    opacity: 1;
  }
}

@keyframes fadOut {
  from {
    visibility: visible;
    opacity: 1;
    transform: scale(1)
  }
  to {
    visibility: hidden;
    opacity: 0;
    transform: scale(0);
  }
}
